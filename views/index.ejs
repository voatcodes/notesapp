<%- include('partials/header')%>
    <main id="createpage">
        <a class="aindex1" id="aindex1" href="/create">Create a note</a>

    <% for (let note of notes) {%>

        <h1 class="h1index"><a class="aindex2" href="/note/<%= note.id %>"><%= note.title %></a></h1>

    <% }%>
    </main>

    <script>
        let titles = document.querySelectorAll('h1')
        titles.forEach(title => {
            title.addEventListener('click', () => {
                location.href = title.firstElementChild.href
            })
        })
    </script>

<%- include('partials/footer')%>    
